Public Class GramáticaBNF {
//Definir Terminales y no terminales
NTerm A;
NTerm EXP;
NTerm N;
Term NUM;
//Token -  terminales
Conj NUM = [0,1,2,3,4,5]
//Definir Gramática 
Gramar operaciones{
	A::= EXP ( ( '+' | '-' ) EXP ) ;
	EXP::= N( ( '*' | '/'  ) N) ;
	A::= [NUM NUM]
	N::= NUM | A;
}
}//fin de la gramática
	Print(operaciones);
}//fin de la clase
